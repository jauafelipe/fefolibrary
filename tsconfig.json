{
  "compilerOptions": {
    "target": "es5",
    "module": "CommonJS",
    "outDir": "./dist",
    "strict": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "baseUrl": "./",
    "paths": {
      "@fefoLibrary/*":["dist/*"]
    }

  },
  "include": ["src/**/*"],
  "exclude": ["node_modules","**/*.test.ts"]
}

